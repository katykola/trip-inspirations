{
  // "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": true, 
    "outDir": "./temp", 
    "rootDir": "./src",
    "module": "esnext",
    "noEmit": false, 

    "moduleResolution": "Bundler",
    "isolatedModules": true,
    "moduleDetection": "force",
    "jsx": "react-jsx",

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true

  },
  "include": ["src", "proxy.js"]
}
